{
	"name": "DFAmesETL",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_AmesProperty_CSV",
						"type": "DatasetReference"
					},
					"name": "AmesPropertyCSV"
				},
				{
					"dataset": {
						"referenceName": "DS_SQL_Amesdbtemp",
						"type": "DatasetReference"
					},
					"name": "SQLAmesdbtemp"
				},
				{
					"dataset": {
						"referenceName": "DS_SQL_FloorDetail",
						"type": "DatasetReference"
					},
					"name": "SQLFloorDetail"
				},
				{
					"dataset": {
						"referenceName": "DS_SQL_SaleProperties",
						"type": "DatasetReference"
					},
					"name": "SQLSaleProperties"
				},
				{
					"dataset": {
						"referenceName": "DS_SQL_MSZoning",
						"type": "DatasetReference"
					},
					"name": "SQLMSZoning"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DS_Output_CSV",
						"type": "DatasetReference"
					},
					"name": "SalidaDatosaCSV"
				}
			],
			"transformations": [
				{
					"name": "joinAmesCSVySQL"
				},
				{
					"name": "aggFloorDetail"
				},
				{
					"name": "joinFloorDetail"
				},
				{
					"name": "convSalesProperties"
				},
				{
					"name": "joinSaleProperties"
				},
				{
					"name": "DerivedCalculationsGrLivArea"
				},
				{
					"name": "DerivedColumYear"
				},
				{
					"name": "ColumnsDerivedFinal"
				},
				{
					"name": "FillCategoricalNulls",
					"description": "Limpieza de valores categóricos "
				},
				{
					"name": "FillNumerics",
					"description": "Limpieza de valores NA en las columnas numéricas"
				},
				{
					"name": "SelectFinal"
				},
				{
					"name": "FillBinaryCols",
					"description": "Se corrige los nulls de las columnas binarias"
				},
				{
					"name": "joinMSZoning"
				}
			],
			"scriptLines": [
				"source(output(",
				"          PID as long,",
				"          {Lot Frontage} as integer,",
				"          {Lot Area} as integer,",
				"          Street as string,",
				"          Alley as string,",
				"          {Lot Shape} as string,",
				"          {Land Contour} as string,",
				"          Utilities as string,",
				"          {Lot Config} as string,",
				"          {Land Slope} as string,",
				"          Neighborhood as string,",
				"          {Condition 1} as string,",
				"          {Condition 2} as string,",
				"          {Bldg Type} as string,",
				"          {House Style} as string,",
				"          {Overall Qual} as integer,",
				"          {Overall Cond} as integer,",
				"          {Year Built} as date,",
				"          {Year Remod/Add} as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> AmesPropertyCSV",
				"source(output(",
				"          {Exter Cond} as long,",
				"          {Exter Qual} as long,",
				"          {Heating QC} as long,",
				"          {Kitchen Qual} as long,",
				"          {MS Zoning} as long,",
				"          pid as long,",
				"          {Roof Style} as string,",
				"          {Roof Matl} as string,",
				"          {Exterior 1st} as string,",
				"          {Exterior 2nd} as string,",
				"          {Mas Vnr Type} as string,",
				"          {Mas Vnr Area} as long,",
				"          foundation as string,",
				"          heating as string,",
				"          {Central Air} as boolean,",
				"          electrical as string,",
				"          {1st Flr SF} as long,",
				"          {2nd Flr SF} as long,",
				"          {Low Qual Fin SF} as long,",
				"          {Kitchen AbvGr} as long,",
				"          {TotRms AbvGrd} as long,",
				"          functional as string,",
				"          fireplaces as long,",
				"          {Fireplace Qu} as string,",
				"          {Paved Drive} as string,",
				"          {Wood Deck SF} as long,",
				"          {Open Porch SF} as long,",
				"          {Enclosed Porch} as long,",
				"          {3Ssn Porch} as long,",
				"          {Screen Porch} as long,",
				"          fence as string,",
				"          {MS SubClass} as long",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> SQLAmesdbtemp",
				"source(output(",
				"          pid as long,",
				"          Floor as long,",
				"          bedrooms as long,",
				"          {Full Bath} as long,",
				"          {Half Bath} as long",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> SQLFloorDetail",
				"source(output(",
				"          pid as long,",
				"          {Sale Date} as timestamp,",
				"          {Sale Type} as string,",
				"          {Sale Condition} as string,",
				"          saleprice as long",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> SQLSaleProperties",
				"source(output(",
				"          id as long,",
				"          description as string,",
				"          code as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> SQLMSZoning",
				"AmesPropertyCSV, SQLAmesdbtemp join(AmesPropertyCSV@PID == SQLAmesdbtemp@pid,",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> joinAmesCSVySQL",
				"SQLFloorDetail aggregate(groupBy(pid),",
				"     TotalBedroom = sum(bedrooms),",
				"          TotalFullBath = sum({Full Bath}),",
				"          TotalHalfBath = sum({Half Bath})) ~> aggFloorDetail",
				"joinAmesCSVySQL, aggFloorDetail join(AmesPropertyCSV@PID == aggFloorDetail@pid,",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> joinFloorDetail",
				"SQLSaleProperties derive(saleprice = toInteger(saleprice)) ~> convSalesProperties",
				"joinFloorDetail, convSalesProperties join(AmesPropertyCSV@PID == SQLSaleProperties@pid,",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> joinSaleProperties",
				"joinMSZoning derive(GrLivArea = toInteger({1st Flr SF}) + toInteger({2nd Flr SF}) + toInteger({Low Qual Fin SF})) ~> DerivedCalculationsGrLivArea",
				"DerivedCalculationsGrLivArea derive(YrSold = year({Sale Date}),",
				"          MoSold = month({Sale Date})) ~> DerivedColumYear",
				"DerivedColumYear derive(GrLivArea = toInteger(coalesce({1st Flr SF}, 0)) +\r",
				"toInteger(coalesce({2nd Flr SF}, 0)) +\r",
				"toInteger(coalesce({Low Qual Fin SF}, 0)),",
				"          YearRemodAdd = coalesce(year({Year Remod/Add}), year({Year Built})),",
				"          Bedroom = coalesce(toInteger(TotalBedroom), 0),",
				"          FullBath = coalesce(toInteger(TotalFullBath), 0),",
				"          HalfBath = coalesce(toInteger(TotalHalfBath), 0)) ~> ColumnsDerivedFinal",
				"ColumnsDerivedFinal derive(Alley_clean = iif(\r",
				"    isNull(Alley) || trim(toString(Alley)) == \"\" || toString(Alley) == \"NA\",\r",
				"    \"Unknown\",\r",
				"    toString(Alley)\r",
				"),",
				"          Fence_clean = iif(\r",
				"    isNull(fence) || trim(fence) == \"\" || lower(fence) == \"na\",\r",
				"    \"None\",\r",
				"    fence\r",
				"),",
				"          Fireplace_Qu_clean = iif(\r",
				"    isNull({Fireplace Qu}) || trim({Fireplace Qu}) == \"\" || lower({Fireplace Qu}) == \"na\",\r",
				"    \"None\",\r",
				"    {Fireplace Qu}\r",
				"),",
				"          Mas_Vnr_Type_clean = iif(\r",
				"    isNull({Mas Vnr Type}) \r",
				"    || trim({Mas Vnr Type}) == \"\" \r",
				"    || lower({Mas Vnr Type}) == \"na\",\r",
				"    \"None\",\r",
				"    {Mas Vnr Type}\r",
				"),",
				"          electrical_clean = iif(\r",
				"    isNull(electrical) \r",
				"    || trim(electrical) == \"\" \r",
				"    || lower(electrical) == \"na\",\r",
				"    \"Unknown\",\r",
				"    electrical\r",
				"),",
				"          Roof_Style_clean = iif(\r",
				"    isNull({Roof Style}) \r",
				"    || trim({Roof Style}) == \"\" \r",
				"    || lower({Roof Style}) == \"na\",\r",
				"    \"Unknown\",\r",
				"    {Roof Style}\r",
				"),",
				"          Roof_Matl_clean = iif(\r",
				"    isNull({Roof Matl}) \r",
				"    || trim({Roof Matl}) == \"\" \r",
				"    || lower({Roof Matl}) == \"na\",\r",
				"    \"None\",\r",
				"    {Roof Matl}\r",
				"),",
				"          Exterior_1st_clean = iif(\r",
				"    isNull({Exterior 1st}) \r",
				"    || trim({Exterior 1st}) == \"\" \r",
				"    || lower({Exterior 1st}) == \"na\",\r",
				"    \"None\",\r",
				"    {Exterior 1st}\r",
				"),",
				"          Exterior_2nd_clean = iif(\r",
				"    isNull({Exterior 2nd}) \r",
				"    || trim({Exterior 2nd}) == \"\" \r",
				"    || lower({Exterior 2nd}) == \"na\",\r",
				"    \"None\",\r",
				"    {Exterior 2nd}\r",
				"),",
				"          foundation_clean = iif(\r",
				"    isNull(foundation) \r",
				"    || trim(foundation) == \"\" \r",
				"    || lower(foundation) == \"na\",\r",
				"    \"None\",\r",
				"    foundation\r",
				"),",
				"          heating_clean = iif(\r",
				"    isNull(heating) \r",
				"    || trim(heating) == \"\" \r",
				"    || lower(heating) == \"na\",\r",
				"    \"None\",\r",
				"    heating\r",
				"),",
				"          functionalClean = iif(\r",
				"    isNull({functional}) \r",
				"    || trim({functional}) == \"\" \r",
				"    || lower({functional}) == \"na\",\r",
				"    \"Unknown\",\r",
				"    {functional}\r",
				")) ~> FillCategoricalNulls",
				"FillCategoricalNulls derive(ExterCondClean = iif(isNull({Exter Cond}) || trim(toString({Exter Cond})) == '' || toString({Exter Cond}) == 'NA', 0L, toLong({Exter Cond})),",
				"          ExterQualClean = iif(isNull({Exter Qual}) || trim(toString({Exter Qual})) == '' || toString({Exter Qual}) == 'NA', 0L, toLong({Exter Qual})),",
				"          HeatingQCClean = iif(isNull({Heating QC}) || trim(toString({Heating QC})) == '' || toString({Heating QC}) == 'NA', 0L, toLong({Heating QC})),",
				"          KitchenQualClean = iif(isNull({Kitchen Qual}) || trim(toString({Kitchen Qual})) == '' || toString({Kitchen Qual}) == 'NA', 0L, toLong({Kitchen Qual})),",
				"          MSZoningClean = iif(isNull({MS Zoning}) || trim(toString({MS Zoning})) == '' || toString({MS Zoning}) == 'NA', 0L, toLong({MS Zoning})),",
				"          MasVnrAreaClean = iif(isNull({Mas Vnr Area}) || trim(toString({Mas Vnr Area})) == '' || toString({Mas Vnr Area}) == 'NA', 0L, toLong({Mas Vnr Area})),",
				"          {1stFlrSFClean} = iif(isNull({1st Flr SF}) || trim(toString({1st Flr SF})) == '' || toString({1st Flr SF}) == 'NA', 0L, toLong({1st Flr SF})),",
				"          {2ndFlrSFClean} = iif(isNull({2nd Flr SF}) || trim(toString({2nd Flr SF})) == '' || toString({2nd Flr SF}) == 'NA', 0L, toLong({2nd Flr SF})),",
				"          TotRmsAbvGrdClean = iif(isNull({TotRms AbvGrd}) || trim(toString({TotRms AbvGrd})) == '' || toString({TotRms AbvGrd}) == 'NA', 0L, toLong({TotRms AbvGrd})),",
				"          fireplacesclean = iif(isNull(fireplaces) || trim(toString(fireplaces)) == '' || toString(fireplaces) == 'NA', 0L, toLong(fireplaces)),",
				"          WoodDeckSFClean = iif(isNull({Wood Deck SF}) || trim(toString({Wood Deck SF})) == '' || toString({Wood Deck SF}) == 'NA', 0L, toLong({Wood Deck SF})),",
				"          OpenPorchSFClean = iif(isNull({Open Porch SF}) || trim(toString({Open Porch SF})) == '' || toString({Open Porch SF}) == 'NA', 0L, toLong({Open Porch SF})),",
				"          EnclosedPorchClean = iif(isNull({Enclosed Porch}) || trim(toString({Enclosed Porch})) == '' || toString({Enclosed Porch}) == 'NA', 0L, toLong({Enclosed Porch})),",
				"          {3SsnPorchClean} = iif(isNull({3Ssn Porch}) || trim(toString({3Ssn Porch})) == '' || toString({3Ssn Porch}) == 'NA', 0L, toLong({3Ssn Porch})),",
				"          ScreenPorchClean = iif(isNull({Screen Porch}) || trim(toString({Screen Porch})) == '' || toString({Screen Porch}) == 'NA', 0L, toLong({Screen Porch})),",
				"          MSSubClassClean = iif(isNull({MS SubClass}) || trim(toString({MS SubClass})) == '' || toString({MS SubClass}) == 'NA', 0L, toLong({MS SubClass})),",
				"          LowQualFinSFClean = iif(isNull({Low Qual Fin SF}) || trim(toString({Low Qual Fin SF})) == '' || toString({Low Qual Fin SF}) == 'NA', 0L, toLong({Low Qual Fin SF})),",
				"          KitchenAbvGrClean = iif(isNull({Kitchen AbvGr}) || trim(toString({Kitchen AbvGr})) == '' || toString({Kitchen AbvGr}) == 'NA', 0L, toLong({Kitchen AbvGr})),",
				"          LotFrontageClean = iif(isNull({Lot Frontage}), 60, {Lot Frontage})) ~> FillNumerics",
				"FillBinaryCols select(mapColumn(",
				"          PID = AmesPropertyCSV@PID,",
				"          MSSubClass = MSSubClassClean,",
				"          MSZoning = MSZoningClean,",
				"          LotFrontage = LotFrontageClean,",
				"          LotArea = {Lot Area},",
				"          Street,",
				"          Alley = Alley_clean,",
				"          LotShape = {Lot Shape},",
				"          LandContour = {Land Contour},",
				"          Utilities,",
				"          LotConfig = {Lot Config},",
				"          LandSlope = {Land Slope},",
				"          Neighborhood,",
				"          Condition1 = {Condition 1},",
				"          Condition2 = {Condition 2},",
				"          BldgType = {Bldg Type},",
				"          HouseStyle = {House Style},",
				"          OverallQual = {Overall Qual},",
				"          OverallCond = {Overall Cond},",
				"          YearBuilt = {Year Built},",
				"          YearRemodAdd,",
				"          RoofStyle = Roof_Style_clean,",
				"          RoofMatl = Roof_Matl_clean,",
				"          Exterior1st = Exterior_1st_clean,",
				"          Exterior2nd = Exterior_2nd_clean,",
				"          MasVnrType = Mas_Vnr_Type_clean,",
				"          MasVnrArea = MasVnrAreaClean,",
				"          ExterQual = ExterQualClean,",
				"          ExterCond = ExterCondClean,",
				"          Foundation = foundation_clean,",
				"          Heating = heating_clean,",
				"          HeatingQC = HeatingQCClean,",
				"          CentralAir = CentralAirClean,",
				"          Electrical = electrical_clean,",
				"          {1stFlrSF} = {1stFlrSFClean},",
				"          {2ndFlrSF} = {2ndFlrSFClean},",
				"          LowQualFinSF = LowQualFinSFClean,",
				"          GrLivArea,",
				"          FullBath,",
				"          HalfBath,",
				"          Bedroom,",
				"          Kitchen = KitchenAbvGrClean,",
				"          KitchenQual = KitchenQualClean,",
				"          TotRmsAbvGrd = TotRmsAbvGrdClean,",
				"          Functional = functionalClean,",
				"          Fireplaces = fireplacesclean,",
				"          FireplaceQu = Fireplace_Qu_clean,",
				"          PavedDrive = PavedDriveClean,",
				"          WoodDeckSF = WoodDeckSFClean,",
				"          OpenPorchSF = OpenPorchSFClean,",
				"          EnclosedPorch = EnclosedPorchClean,",
				"          {3SsnPorch} = {3SsnPorchClean},",
				"          ScreenPorch = ScreenPorchClean,",
				"          Fence = Fence_clean,",
				"          MoSold,",
				"          YrSold,",
				"          SaleType = {Sale Type},",
				"          SaleCondition = {Sale Condition},",
				"          SalePrice = saleprice",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> SelectFinal",
				"FillNumerics derive(CentralAirClean = iif(isNull({Central Air}) || trim(toString({Central Air})) == '' || toString({Central Air}) == 'NA' || toString({Central Air}) == 'N', 'False', 'True'),",
				"          PavedDriveClean = iif(isNull({Paved Drive}) || trim(toString({Paved Drive})) == '' || toString({Paved Drive}) == 'NA', 'N', toString({Paved Drive}))) ~> FillBinaryCols",
				"joinSaleProperties, SQLMSZoning join({MS Zoning} == id,",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> joinMSZoning",
				"SelectFinal sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          {PID,MSSubClass,MSZoning,LotFrontage,LotArea,Street,Alley,LotShape,LandContour,Utilities,LotConfig,LandSlope,Neighborhood,Condition1,Condition2,BldgType,HouseStyle,OverallQual,OverallCond,YearBuilt,YearRemodAdd,RoofStyle,RoofMatl,Exterior1st,Exterior2nd,MasVnrType,MasVnrArea,ExterQual,ExterCond,Foundation,Heating,HeatingQC,CentralAir,Electrical,1stFlrSF,2ndFlrSF,LowQualFinSF,GrLivArea,FullBath,HalfBath,Bedroom,Kitchen,KitchenQual,TotRmsAbvGrd,Functional,Fireplaces,FireplaceQu,PavedDrive,WoodDeckSF,OpenPorchSF,EnclosedPorch,3SsnPorch,ScreenPorch,Fence,MoSold,YrSold,SaleType,SaleCondition,SalePrice} as string",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> SalidaDatosaCSV"
			]
		}
	}
}