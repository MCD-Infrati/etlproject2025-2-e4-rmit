{
	"name": "DFAmesETL",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_AmesProperty_CSV",
						"type": "DatasetReference"
					},
					"name": "AmesPropertyCSV"
				},
				{
					"dataset": {
						"referenceName": "DS_SQL_Amesdbtemp",
						"type": "DatasetReference"
					},
					"name": "SQLAmesdbtemp"
				},
				{
					"dataset": {
						"referenceName": "DS_SQL_FloorDetail",
						"type": "DatasetReference"
					},
					"name": "SQLFloorDetail"
				},
				{
					"dataset": {
						"referenceName": "DS_SQL_SaleProperties",
						"type": "DatasetReference"
					},
					"name": "SQLSaleProperties"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "join1"
				},
				{
					"name": "aggFloorDetail"
				},
				{
					"name": "join2"
				},
				{
					"name": "convSalesProperties"
				},
				{
					"name": "join3"
				},
				{
					"name": "derivedCalculations1"
				},
				{
					"name": "DerivedColumYear"
				},
				{
					"name": "ColumnsDerivedFinal"
				},
				{
					"name": "FillCategoricalNulls",
					"description": "Limpieza de valores categÃ³ricos "
				},
				{
					"name": "AggModeCountExterCond"
				},
				{
					"name": "WindowModeExterCond"
				},
				{
					"name": "FilterModeExterCond"
				},
				{
					"name": "join4"
				},
				{
					"name": "ExterCondClean"
				}
			],
			"scriptLines": [
				"source(output(",
				"          PID as long,",
				"          {Lot Frontage} as integer,",
				"          {Lot Area} as integer,",
				"          Street as string,",
				"          Alley as string,",
				"          {Lot Shape} as string,",
				"          {Land Contour} as string,",
				"          Utilities as string,",
				"          {Lot Config} as string,",
				"          {Land Slope} as string,",
				"          Neighborhood as string,",
				"          {Condition 1} as string,",
				"          {Condition 2} as string,",
				"          {Bldg Type} as string,",
				"          {House Style} as string,",
				"          {Overall Qual} as integer,",
				"          {Overall Cond} as integer,",
				"          {Year Built} as date,",
				"          {Year Remod/Add} as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> AmesPropertyCSV",
				"source(output(",
				"          {Exter Cond} as long,",
				"          {Exter Qual} as long,",
				"          {Heating QC} as long,",
				"          {Kitchen Qual} as long,",
				"          {MS Zoning} as long,",
				"          pid as long,",
				"          {Roof Style} as string,",
				"          {Roof Matl} as string,",
				"          {Exterior 1st} as string,",
				"          {Exterior 2nd} as string,",
				"          {Mas Vnr Type} as string,",
				"          {Mas Vnr Area} as long,",
				"          foundation as string,",
				"          heating as string,",
				"          {Central Air} as boolean,",
				"          electrical as string,",
				"          {1st Flr SF} as long,",
				"          {2nd Flr SF} as long,",
				"          {Low Qual Fin SF} as long,",
				"          {Kitchen AbvGr} as long,",
				"          {TotRms AbvGrd} as long,",
				"          functional as string,",
				"          fireplaces as long,",
				"          {Fireplace Qu} as string,",
				"          {Paved Drive} as string,",
				"          {Wood Deck SF} as long,",
				"          {Open Porch SF} as long,",
				"          {Enclosed Porch} as long,",
				"          {3Ssn Porch} as long,",
				"          {Screen Porch} as long,",
				"          fence as string,",
				"          {MS SubClass} as long",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> SQLAmesdbtemp",
				"source(output(",
				"          pid as long,",
				"          Floor as long,",
				"          bedrooms as long,",
				"          {Full Bath} as long,",
				"          {Half Bath} as long",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> SQLFloorDetail",
				"source(output(",
				"          pid as long,",
				"          {Sale Date} as timestamp,",
				"          {Sale Type} as string,",
				"          {Sale Condition} as string,",
				"          saleprice as long",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> SQLSaleProperties",
				"AmesPropertyCSV, SQLAmesdbtemp join(AmesPropertyCSV@PID == SQLAmesdbtemp@pid,",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1",
				"SQLFloorDetail aggregate(groupBy(pid),",
				"     TotalBedroom = sum(bedrooms),",
				"          TotalFullBath = sum({Full Bath}),",
				"          TotalHalfBath = sum({Half Bath})) ~> aggFloorDetail",
				"join1, aggFloorDetail join(AmesPropertyCSV@PID == aggFloorDetail@pid,",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join2",
				"SQLSaleProperties derive(saleprice = toInteger(saleprice)) ~> convSalesProperties",
				"join2, convSalesProperties join(AmesPropertyCSV@PID == SQLSaleProperties@pid,",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join3",
				"join3 derive(GrLivArea = toInteger({1st Flr SF}) + toInteger({2nd Flr SF}) + toInteger({Low Qual Fin SF})) ~> derivedCalculations1",
				"derivedCalculations1 derive(YrSold = year({Sale Date}),",
				"          MoSold = month({Sale Date})) ~> DerivedColumYear",
				"DerivedColumYear derive(GrLivArea = toInteger(coalesce({1st Flr SF}, 0)) +\r",
				"toInteger(coalesce({2nd Flr SF}, 0)) +\r",
				"toInteger(coalesce({Low Qual Fin SF}, 0)),",
				"          YearRemodAdd = coalesce(year({Year Remod/Add}), year({Year Built})),",
				"          Bedroom = coalesce(toInteger(TotalBedroom), 0),",
				"          FullBath = coalesce(toInteger(TotalFullBath), 0),",
				"          HalfBath = coalesce(toInteger(TotalHalfBath), 0)) ~> ColumnsDerivedFinal",
				"ColumnsDerivedFinal derive(Alley_clean = iif(\r",
				"    isNull(Alley) || trim(toString(Alley)) == \"\" || toString(Alley) == \"NA\",\r",
				"    \"Unknown\",\r",
				"    toString(Alley)\r",
				"),",
				"          Fence_clean = iif(\r",
				"    isNull(fence) || trim(fence) == \"\" || lower(fence) == \"na\",\r",
				"    \"None\",\r",
				"    fence\r",
				"),",
				"          Fireplace_Qu_clean = iif(\r",
				"    isNull({Fireplace Qu}) || trim({Fireplace Qu}) == \"\" || lower({Fireplace Qu}) == \"na\",\r",
				"    \"None\",\r",
				"    {Fireplace Qu}\r",
				"),",
				"          Mas_Vnr_Type_clean = iif(\r",
				"    isNull({Mas Vnr Type}) \r",
				"    || trim({Mas Vnr Type}) == \"\" \r",
				"    || lower({Mas Vnr Type}) == \"na\",\r",
				"    \"None\",\r",
				"    {Mas Vnr Type}\r",
				"),",
				"          electrical_clean = iif(\r",
				"    isNull(electrical) \r",
				"    || trim(electrical) == \"\" \r",
				"    || lower(electrical) == \"na\",\r",
				"    \"Unknown\",\r",
				"    electrical\r",
				"),",
				"          Roof_Style_clean = iif(\r",
				"    isNull({Roof Style}) \r",
				"    || trim({Roof Style}) == \"\" \r",
				"    || lower({Roof Style}) == \"na\",\r",
				"    \"Unknown\",\r",
				"    {Roof Style}\r",
				"),",
				"          Roof_Matl_clean = iif(\r",
				"    isNull({Roof Matl}) \r",
				"    || trim({Roof Matl}) == \"\" \r",
				"    || lower({Roof Matl}) == \"na\",\r",
				"    \"None\",\r",
				"    {Roof Matl}\r",
				"),",
				"          Exterior_1st_clean = iif(\r",
				"    isNull({Exterior 1st}) \r",
				"    || trim({Exterior 1st}) == \"\" \r",
				"    || lower({Exterior 1st}) == \"na\",\r",
				"    \"None\",\r",
				"    {Exterior 1st}\r",
				"),",
				"          Exterior_2nd_clean = iif(\r",
				"    isNull({Exterior 2nd}) \r",
				"    || trim({Exterior 2nd}) == \"\" \r",
				"    || lower({Exterior 2nd}) == \"na\",\r",
				"    \"None\",\r",
				"    {Exterior 2nd}\r",
				"),",
				"          foundation_clean = iif(\r",
				"    isNull(foundation) \r",
				"    || trim(foundation) == \"\" \r",
				"    || lower(foundation) == \"na\",\r",
				"    \"None\",\r",
				"    foundation\r",
				"),",
				"          heating_clean = iif(\r",
				"    isNull(heating) \r",
				"    || trim(heating) == \"\" \r",
				"    || lower(heating) == \"na\",\r",
				"    \"None\",\r",
				"    heating\r",
				")) ~> FillCategoricalNulls",
				"FillCategoricalNulls aggregate(groupBy(Neighborhood,",
				"          {Exter Cond}),",
				"     cnt = count({Exter Cond})) ~> AggModeCountExterCond",
				"AggModeCountExterCond window(over(Neighborhood),",
				"     desc(cnt, true),",
				"     asc({Exter Cond}, true),",
				"     Mode_ExterCond = rowNumber()) ~> WindowModeExterCond",
				"WindowModeExterCond filter(Mode_ExterCond == 1",
				") ~> FilterModeExterCond",
				"FillCategoricalNulls, FilterModeExterCond join(AmesPropertyCSV@Neighborhood == AggModeCountExterCond@Neighborhood,",
				"     joinType:'left',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join4",
				"join4 derive(ExterCondClean = iif(\r",
				"    isNull({Exter Cond}),\r",
				"    Mode_ExterCond,\r",
				"    {Exter Cond}\r",
				")\r",
				"\r",
				") ~> ExterCondClean"
			]
		}
	}
}